{
  "environment": "stage",
  "browserName": "chrome",
  "isRemote": false,
  "isElementHighlightEnabled": true,

  "driverSettings": {
    "chrome": {
      "webDriverVersion": "latest",
      "capabilities": {
        "enableVNC": true
      },
      "options": {
        "intl.accept_languages": "en",
        "safebrowsing.enabled": "true",
        "profile.default_content_settings.popups": "0",
        "disable-popup-blocking": "true",
        "download.prompt_for_download": "false",
        "download.default_directory": ".//target//downloads//"
      },
      "startArguments": [
        "window-size=1920,1080",
        "--no-sandbox",
        "--disable-popup-blocking",
        "--disable-gpu"
      ]
    }
  },
  "timeouts": {
    "timeoutImplicit": 0,
    "timeoutCondition": 30,
    "timeoutScript": 20,
    "timeoutPageLoad": 30,
    "timeoutPollingInterval": 300,
    "timeoutCommand": 120
  },
  "retry": {
    "number": 2,
    "pollingInterval": 300
  },
  "logger": {
    "language": "en"
  },
  "elementCache": {
    "isEnabled": false
  }
}
